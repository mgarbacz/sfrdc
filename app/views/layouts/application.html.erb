<!doctype html>
<!-- Autogenerated pieces, please don't judge on tidiness! -->
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= content_for?(:title) ? yield(:title) :
    'State Farm Research and Development Center' %></title>
  <%= stylesheet_link_tag    'application', :media => 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
  <%= yield(:head) %>
</head>
<body>
  <%= render 'layouts/header' %>

  <div id="page">
    <div class="container">
      <div class="notice" style="background:green;">
        <%= flash[:notice] %>
      </div>

      <%= yield %>

    </div><!-- end .container -->
  </div><!-- end #page -->

  <%= render 'layouts/footer' %>
</body>
</html>
