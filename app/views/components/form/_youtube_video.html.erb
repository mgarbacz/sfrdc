<div class="control-group">
  <%= f.label :video_url, :class => 'control-label' %>
  <div class="controls">
    <%= f.text_field :video_url %>
  </div>
</div>
<div class="control-group">
  <%= f.label :size, :class => 'control-label' %>
  <div class="controls">
    <%= f.select(:size, ['3', '6', '9', '12']) %>
  </div>
</div>
